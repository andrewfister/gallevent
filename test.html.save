            myOptions);

	var image = 'img/pin-map-dining.png';
    	var myLatLng = new google.maps.LatLng(-33.890542, 151.274856);
    	var beachMarker = new google.maps.Marker({
        position: myLatLng,
        map: map,
        icon: image
    });


      }

      google.maps.event.addDomListener(window, 'load', initialize);
</script>
